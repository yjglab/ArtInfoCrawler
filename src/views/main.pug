include mixins/printInfo

doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title #{pageTitle}
    body 
        ul 
            h2 British Museum
            +printInfo(britishMsmInfo)
            h2 Louvre Museum
            +printInfo(louvreMsmInfo)
            h2 Pompidou Museum
            +printInfo(pompidouMsmInfo)
            h2 London National Gallery
            +printInfo(londonNatlInfo)
            h2 Wien Museum
            +printInfo(wienMsmInfo)
            