include mixins/printInfo

doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        style.
            img {
                width: 300px;
                object-fit: cover;
            }
            .container {
                display: grid;
                
                grid-template-columns: 1fr 1fr 1fr;
                grid-template-rows: repeat(20, 600px);
                row-gap: 10px;
                column-gap: 20px;
            }
            .container > div {
                margin: 40px;
            }
            .container > div .exb {
                height: 100%;
                overflow: auto;
            }

        title #{pageTitle}
    body 
        div(class="container")
            div
                h3 游비 AT. Austria 游비
                +printInfo(at)
            div
                h3 游비 ES. Spain 游비
                +printInfo(es)
            div
                h3 游비 FR. French 游비 
                +printInfo(fr)
            div
                h3 游비 IT. Italy 游비
                +printInfo(it)
            div
                h3 游비 UK. United Kingdom 游비
                +printInfo(uk)
            div
                h3 游비 US. United States 游비
                +printInfo(us)
            div
                h3 游비 KR. Republic of Korea 游비
                +printInfo(kr)
            div
                h3 游비 JP. Japan 游비
                +printInfo(jp)
            div
                h3 游비 GR. Greece 游비
                +printInfo(gr)
            div
                h3 游비 CZ. Czech 游비
                +printInfo(cz)
            div
                h3 游비 RU. Russia 游비
                +printInfo(ru)
            div
                h3 游비 CH. Swiss 游비
                +printInfo(ch)
            div
                h3 游비 PL. Poland 游비
                +printInfo(pl)
            div
                h3 游비 CA. Canada 游비
                +printInfo(ca)
            div
                h3 游비 AU. Austrailia 游비
                +printInfo(au)
            div
                h3 游비 TR. Turkey 游비
                +printInfo(tr)
            div
                h3 游비 DK. Denmark 游비
                +printInfo(dk)

    script(src="/static/js/main.js")